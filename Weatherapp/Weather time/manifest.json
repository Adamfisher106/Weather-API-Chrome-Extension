{
   "author": "Adam Fisher",
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Click to open Weather information"
   },
   "description": "This checks the weather were you are.",
   "manifest_version": 2,
   "name": "Weather in Winch",
   "optional_permissions": [ "tabs" ],
   "permissions": [ "tabs", "geolocation", "storage", "http://*/", "https://*/" ],
   "version": "1.0.0",
   "version_name": "1.0.0"
}